{
  "expo": {
    "name": "trial-app",
    "slug": "trial-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "release": {
        "minify": false
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F1112"
      },
      "package": "com.whiskas101.trialapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/firestarter.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0F1112"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gotham Rounded Medium.ttf",
            "./assets/fonts/Gotham Rounded Medium Italic.ttf",
            "./assets/fonts/Gotham Rounded Bold.ttf",
            "./assets/fonts/Gotham Rounded Bold Italic.ttf",
            "./assets/fonts/Gotham Rounded Light.ttf",
            "./assets/fonts/Gotham Rounded Light Italic.ttf",
            "./assets/fonts/Gotham Rounded Book.ttf",
            "./assets/fonts/Gotham Rounded Book Italic.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54045441-6313-495d-bb0f-18e85de18d4b"
      }
    }
  }
}
